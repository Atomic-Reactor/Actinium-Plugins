    "sharp": "^0.25.4",
    "slugify": "^1.4.5",
    "socket.io": "^3.1.0",
    "string-to-arraybuffer": "^1.0.2",
    "tree-flatten": "^1.0.0",
    "underscore": "^1.10.2",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@atomic-reactor/cli": "^2.2.31",
    "action-sequence": "^1.1.1",
    "chalk": "^2.4.2",
    "eslint": "^5.13.0",
    "fs-extra": "^9.1.0",
    "husky": "^3.0.9",
    "inquirer": "^8.0.0",
    "lint-staged": "^9.4.2",
    "node-clipboard": "^1.2.0",
    "nodemon": "^2.0.4",
    "prettier": "^1.18.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Atomic-Reactor/Actinium-2.0"
  },
  "engines": {
    "node": "12.13.0",
    "npm": ">=6.12.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "actiniumDependencies": {}
}
