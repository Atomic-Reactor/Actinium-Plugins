    "publish:actinium-pulse": "cd ./actinium_modules/@atomic-reactor/actinium-pulse && arcli publish && cd $INIT_CWD",
    "publish:actinium-recycle": "cd ./actinium_modules/@atomic-reactor/actinium-recycle && arcli publish && cd $INIT_CWD",
