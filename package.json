{
  "name": "@atomic-reactor/actinium-plugins",
  "version": "3.6.6",
  "author": "Reactium LLC",
  "private": true,
  "scripts": {
    "start": "node ./src/index.js",
    "docker": "npm-run-all -s docker:*",
    "docker:build": "docker image build -t atomicreactor/actinium:$npm_package_version .",
    "docker:push": "docker push atomicreactor/actinium:$npm_package_version",
    "docs": "arcli docs -s .core,src/app,node_modules/@atomic-reactor -d docs",
    "local": "nodemon",
    "publish-all": "npm-run-all -s publish:actinium*",
    "publish:actinium-block": "cd ./actinium_modules/@atomic-reactor/actinium-block && arcli publish && cd $INIT_CWD",
    "publish:actinium-blueprint": "cd ./actinium_modules/@atomic-reactor/actinium-blueprint && arcli publish && cd $INIT_CWD",
    "publish:actinium-capability": "cd ./actinium_modules/@atomic-reactor/actinium-capability && arcli publish && cd $INIT_CWD",
