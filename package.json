    "https": "^1.0.0",
    "js-beautify": "^1.11.0",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "kerberos": "1.1.3",
    "lunr": "^2.3.8",
    "memory-cache": "^0.2.0",
    "moment": "*",
    "mongodb": "^3.6.0",
    "morgan": "^1.10.0",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.4.11",
    "nodemailer-mailgun-transport": "^2.0.0",
    "npm-run-all": "^4.1.5",
