    "publish:actinium-taxonomy": "cd ./actinium_modules/@atomic-reactor/actinium-taxonomy && arcli publish && cd $INIT_CWD",
    "publish:actinium-type": "cd ./actinium_modules/@atomic-reactor/actinium-type && arcli publish && cd $INIT_CWD",
    "publish:actinium-users": "cd ./actinium_modules/@atomic-reactor/actinium-users && arcli publish && cd $INIT_CWD",
    "publish:actinium-wizard": "cd ./actinium_modules/@atomic-reactor/actinium-wizard && arcli publish && cd $INIT_CWD"
